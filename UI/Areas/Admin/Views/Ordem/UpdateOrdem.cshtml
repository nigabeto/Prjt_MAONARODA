
@{
    ViewBag.Title = "UpdateOrdem";
    Layout = "~/Areas/Admin/Views/Shared/AdimnLayout.cshtml";
}

<section class="content-header">
    <h1>Atualizar Ordem de Serviço</h1>
    <ol class="breadcrumb float-sm-left">
        <li class="breadcrumb-item"><a>Admin</a></li>
        <li class="breadcrumb-item"><a>Ordem</a></li>
        <li class="breadcrumb-item active"><a>Atualizar Ordem</a></li>
    </ol>
</section>
<br />
<div class="card-footer">
    @*<button type="submit" class="btn btn-primary" href="/Admin/Ordem/OrdemList">Lista de Ordens</button>*@
    <button type="submit" class="btn btn-primary" onclick="window.location.href='/Admin/Ordem/OrdemList'">Lista de ordens</button>
</div>
<section class="content">
    <div class="row">
        <div class="col-md-9">
            @Html.Partial("~/Areas/Admin/Views/Shared/MessageForm.cshtml")
            <div class="box box-primary">
                @using (Html.BeginForm("UpdateOrdem", "Ordem", FormMethod.Post, new { @enctype = "multipart/form-data" }))
                {
                    @Html.Partial("Partial/OrdemForm")
                }

            </div>
        </div>
    </div>
</section>

